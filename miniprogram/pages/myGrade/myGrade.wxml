<view class="container">
    <view>
        <view class="session">
            <view  class="goodsList"  wx:for="{{goodList}}">
                <view class="goodsImg">
                    <image class="goods-img" src="{{item.src}}"></image>   
                </view>
                <view class="goodsRight">
                    <view class="goodsTitle">{{item.goods_name}}</view>
                    <view class="goodsUser">{{item.pintuan_num}}人团 立减￥元</view>
                    <view class="goodsDet">
                        <view>
                            <view class="price">￥{{item.goods_price}}</view>
                            <view class="bookNum">已团{{item.sales_num}}件</view>
                        </view>
                        <view class="toBook">
              去开团
            </view>
                    </view>
                </view>
            </view>
        </view>
    
    </view>
</view>
