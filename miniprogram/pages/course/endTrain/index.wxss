/* miniprogram/pages/course/endTrain/index.wxss */

button::after {
  border: none;
}

.c-wrapper,.a-wrapper {
  margin-bottom: 24rpx;
  background: #ffffff;
}

.c-list-box {
  padding: 0 28rpx;
}

.a-link-box {
  padding: 0 28rpx 28rpx;
}

.a-link-box>image {
  width: 100%;
  height: 250rpx;
}

.d-wrapper {
  padding: 0 28rpx;
  background: #fafafa;
}

.title {
  margin: 38rpx 0 40rpx 28rpx;
  font-size: 28rpx;
  color: #333333;
}

.bg {
  background-color: #FFFFFF;
}

.nav {
  margin-bottom: 24rpx;
}

.end-title {
  height: 59rpx;
  font-size: 42rpx;
  text-align: center;
  color: #333333;
  margin: 100rpx 0 38rpx;
}

.end-main {
  text-align: center;
  height: 42rpx;
  font-size: 30rpx;
  color: #666;
  margin-bottom: 60rpx;
}

.download-btn {
  width: 566rpx;
  height: 100rpx;
  line-height: 100rpx;
  background: #24C789;
  border-radius: 100px;
  font-size: 36rpx;
  color: #FFFFFF;
  text-align: center;
  margin-bottom: 56rpx;
  margin-top: 20rpx;
}

.img-box {
  text-align: center;
  display: flex;
  justify-content: center;
}

.img {
  width: 90rpx;
  height: 134rpx;
  margin-top: 80rpx;
}

.over-title {
  height: 59rpx;
  font-size: 42rpx;
  text-align: center;
  color: #333333;
  margin: 71rpx 0 38rpx;
}

.modal-tip {
  line-height: 1;
}
.write
{
  margin-top: 50px;
}
.comment
{
  width: 80%;
  margin-left: 50rpx;
  height: 120rpx;
  margin-top: 10rpx;
  border-radius: 10rpx;
  border:2px solid #eee;
}