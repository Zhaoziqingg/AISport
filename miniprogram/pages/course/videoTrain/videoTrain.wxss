page {
  overflow-y: none!important;
}

video {
  width: 100%;
  height: 100%;
}

button::after {
  border: none;
}

.box {
  background-color: #fafafa;
  width: 100%;
  height: 100%;
}

.box .own-video {
  width: 100%;
  height: 550rpx;
  position: fixed;
  top: 0;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}

.title
{
  margin: 20rpx;
  font-size: 12px;
  color: #444;
}

.intro
{
  margin-left: 20rpx;
  font-size: 14px;
  color: rgb(20, 20, 20);
}


.over-btn {
  width: 566rpx;
  height: 100rpx;
  line-height: 100rpx;
  background: #24C789;
  border-radius: 100px;
  font-size: 36rpx;
  color: #FFFFFF;
  text-align: center;
  margin-top: 800rpx;
}







