page {
  overflow-y: none!important;
}

video {
  width: 100%;
  height: 100%;
}

button::after {
  border: none;
}

.box {
  background-color: #fafafa;
  width: 100%;
  height: 100%;
}

.box .own-video {
  width: 100%;
  height: 422rpx;
  position: fixed;
  top: 0;
  overflow: hidden;
  z-index: 9999;
  display: flex;
  flex-direction: column;
}

.mains {
  position: relative;
  padding-top: 422rpx;
}

.mains .nav {
  margin-bottom: 24rpx;
  background: #ffffff;
  padding-bottom: 28rpx;
}

.mains .audio-desc {
  position: relative;
  overflow: hidden;
}

.mains .train-name {
  height: 50rpx;
  font-size: 36rpx;
  color: #333333;
  text-align: left;
  margin: 35rpx 0 35rpx 28rpx;
  display: inline-block;
  font-family: PingFangSC-Medium;
}



.mains .main .title {
  padding: 40rpx 28rpx;
  font-size: 28rpx;
  line-height: 40rpx;
}

.over-btn {
  width: 566rpx;
  height: 100rpx;
  line-height: 100rpx;
  background: #24C789;
  border-radius: 100px;
  font-size: 36rpx;
  color: #FFFFFF;
  text-align: center;
  margin-top: 600rpx;
}

.mains .main .ad {
  padding: 28rpx;
}

.step {
  padding: 0 28rpx;
}

.wxParse-h4 {
  font-size: 26rpx;
  color: #333333;
  line-height: 44rpx;
  font-family: PingFangSC-Medium;
  margin: 28rpx 0 0 0;
}

.wxParse-ul {
  margin: 0;
}

.wxParse-li,.wxParse-li-inner {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #666666;
  margin: 0;
  line-height: 44rpx;
}

.wxParse-li-circle {
  width: 6rpx;
  height: 6rpx;
  border-radius: 50%;
  margin-right: 15rpx;
  vertical-align: middle;
  border: none;
  background: #666666;
}

.on-title {
  position: absolute;
  top: 22rpx;
  left: 20rpx;
  font-size: 28rpx;
  color: #24C789;
  line-height: 44rpx;
  z-index: 10;
}

.on-time {
  position: absolute;
  bottom: 22rpx;
  left: 20rpx;
  font-size: 28rpx;
  color: #24C789;
  line-height: 44rpx;
  z-index: 10;
}

.wxParse-li,.wxParse-li-inner,.wxParse-li-text {
  color: #666666;
}